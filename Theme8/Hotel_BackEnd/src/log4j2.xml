<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appenders>
		<File name="logFile" fileName="logFileBACKEND.log">
			<PatternLayout pattern="%d [%t] %5p: %logger{36} (%M:%L) - %m%n" />
		</File>
	</appenders>

	<loggers>
		<Root level="error" additivity="false">
			<AppenderRef ref="logFile" />
		</Root>
	</loggers>

</configuration>